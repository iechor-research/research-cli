{
  "name": "research-cli",
  "version": "0.2.6",
  "bin": "packages/cli/dist/index.js",
  "pkg": {
    "targets": [
      "node18-linux-x64",
      "node18-macos-x64",
      "node18-macos-arm64",
      "node18-win-x64"
    ],
    "outputPath": "dist-pkg",
    "assets": [
      "packages/cli/dist/**/*",
      "packages/core/dist/**/*",
      "packages/cli/src/utils/sandbox-*.sb",
      "node_modules/@google/genai/dist/**/*",
      "node_modules/llm-interface/dist/**/*"
    ],
    "scripts": [
      "packages/cli/dist/**/*.js",
      "packages/core/dist/**/*.js"
    ]
  }
} 